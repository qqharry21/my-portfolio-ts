{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "printWidth": 100,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "requirePragma": false,
  "insertPragma": false,
  "singleAttributePerLine": true,
  "tailwindFunctions": ["cn", "cva"],
  "plugins": ["prettier-plugin-tailwindcss"]
}
